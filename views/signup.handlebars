<h2>Sign Up!</h2>

<form id="form-group">
 <div class="form-group">
    <label for="exampleInputUser1">Username</label>
    <input type="username" class="form-control" id="exampleInputUser1" aria-describedby="emailHelp" placeholder="Please enter a username">
  </div>
  <div class="form-group">
    <label for="exampleInputEmail1">Email address</label>
    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Please enter an email">
    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Password</label>
    <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Please enter a password">
  </div>
  <div class="form-group form-check">
    <small><a href="/login">Already have an account?</a></small>
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>

<script src="/js/signup.js"></script>